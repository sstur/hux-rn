{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["dom", "esnext"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
